   "source": [
    "# stack is basically a list but its last in, first out principle. only append and pop from the list\n",
    "class Stack:\n",
    "    def __init__(self):\n",
    "        self.stack = []\n",
    "\n",
    "    def push(self, n):\n",
    "        self.stack.append(n)\n",
    "\n",
    "    def pop(self):\n",
    "        return self.stack.pop()\n"
   ]
